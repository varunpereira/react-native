{
  "plugins": [
    [
      "module:react-native-dotenv",
      {
        "moduleName": "@env",
        "path": ".env",
        "blacklist": null,
        "whitelist": null,
        "safe": false,
        "allowUndefined": true
      }
    ],
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "@src": "./src"
        }
      }
    ]
  ]
}
